# ident	"%Z%%M%	%I%	%E% SMI"
#
# SUNWvia823x preremove script

PATH=/usr/bin:/usr/sbin:${PATH}
export PATH

EXIT=0

installed() {
	driver=$1
	grep "^${driver} " $BASEDIR/etc/name_to_major > /dev/null 2>&1
	if [ "$?" -eq 0 ]; then
		return 1
	else
		return 0
	fi
}

case "${ARCH}" in
	i386)
		installed audiovia823x || rem_drv -b ${BASEDIR} audiovia823x || EXIT=1
	;;
esac

exit ${EXIT}
